<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.AndroidComposeCodingTemplate" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:statusBarColor">@color/purple_700</item>
    </style>

    <style name="CalendarViewCustomLight" parent="android:Widget.CalendarView">
        <item name="android:colorAccent">@color/asseco_blue</item>
        <item name="android:textColorPrimary">@color/black</item>
        <item name="android:textColorSecondary">@color/gray</item>
    </style>

    <style name="CalendarViewCustomDark" parent="android:Widget.CalendarView">
        <item name="android:colorAccent">@color/asseco_blue</item>
        <item name="android:textColorPrimary">@color/white</item>
        <item name="android:textColorSecondary">@color/white</item>
    </style>

</resources>